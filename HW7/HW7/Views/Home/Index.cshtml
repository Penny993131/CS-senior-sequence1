    <div class="col-md-8">

        <dl class="dl-horizontal">
            <dt>
                @Html.Label("Name:")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.Label("Owner:")
            </dt>

